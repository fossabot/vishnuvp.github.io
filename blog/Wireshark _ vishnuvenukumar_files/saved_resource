	font-weight: bold;
}

dfn,
cite,
em,
i {
	font-style: italic;
}

blockquote {
	background: #f9f9f9;
	border: 3px solid #eee;
	color: #666;
	font-style: italic;
	font-weight: 300;
	margin: 0 0 1.5em;
	padding: 1.5em;
}

blockquote blockquote {
	background: transparent;
	border: 0;
	padding-bottom: 0;
	padding-right: 0;
}

blockquote p:last-of-type {
	margin: 0;
}

address {
	margin: 0 0 1.5em;
}

pre {
	background: #eee;
	font-family: "Courier 10 Pitch", Courier, monospace;
	font-size: 15px;
	font-size: 1.5rem;
	line-height: 1.6;
	margin-bottom: 1.6em;
	max-width: 100%;
	overflow: auto;
	padding: 1.6em;
}

code,
kbd,
tt,
var {
	font: 15px Monaco, Consolas, "Andale Mono", "DejaVu Sans Mono", monospace;
}

